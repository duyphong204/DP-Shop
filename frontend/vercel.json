{
  "version":2,
  "builds":[
      {
          "src":"package.json",
          "use":"@vercel/static-build",
          "config":{"distDir":"dist"}
      }
  ],
  
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
    "buildCommand": "cd frontend && npm install && npm run build"

}